{
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint", "import"],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:import/typescript"
    ],
    "rules": {
      "import/order": ["error", { "alphabetize": { "order": "asc" } }]
    }
  }
  
